{
  "name": "beastcss",
  "version": "1.2.1",
  "description": "Inline critical CSS and async load the rest.",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.esm.js"
  },
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "Freddy Escobar <freddyescobar@hotmail.fr>",
  "keywords": [
    "critical css",
    "inline css",
    "critical",
    "beastcss",
    "performance",
    "fast"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/freddy38510/beastcss",
    "directory": "packages/beastcss"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rimraf dist && rollup -c -w"
  },
  "dependencies": {
    "@freddy38510/dropcss": "2.4.0",
    "chalk": "4.1.2",
    "fast-glob": "3.2.11",
    "node-html-parser": "5.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
